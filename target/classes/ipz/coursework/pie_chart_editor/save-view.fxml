<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="110.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="saveText" alignment="TOP_CENTER" layoutY="14.0" prefHeight="53.0" prefWidth="304.0" text="Зберегти змніни в &quot; &quot;?" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button fx:id="save" layoutX="14.0" layoutY="70.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="85.0" text="Зберегти" textAlignment="CENTER" />
      <Button fx:id="dontSave" layoutX="109.0" layoutY="70.0" mnemonicParsing="false" prefWidth="85.0" text="Не зберігати" textAlignment="CENTER" />
      <Button fx:id="cancel" layoutX="204.0" layoutY="70.0" mnemonicParsing="false" prefWidth="85.0" text="Скасувати" textAlignment="CENTER" />
   </children>
</AnchorPane>
