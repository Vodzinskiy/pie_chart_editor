<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="110.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="saveText" alignment="TOP_CENTER" layoutY="14.0" prefHeight="53.0" prefWidth="304.0" text="Зберегти змніни в &quot; &quot;?" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="save" layoutX="14.0" layoutY="72.0" mnemonicParsing="false" text="Зберегти" />
      <Button fx:id="dontSave" layoutX="109.0" layoutY="72.0" mnemonicParsing="false" text="Не зберігати" />
      <Button fx:id="cancel" layoutX="218.0" layoutY="72.0" mnemonicParsing="false" text="Скасувати" />
   </children>
</AnchorPane>
